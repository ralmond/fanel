\name{nsubj}
\alias{nsubj}
\alias{nsubj<-}
\alias{nocc}
\alias{nocc<-}
\alias{maxocc}
\alias{maxocc<-}
\alias{minocc}
\alias{minocc<-}

\title{Accesses shape information about Panamt, Panel_Frame, and Panel_Data}
\description{

  Accesses the number of subject or occassions for a fanel data object.
  For objects where the true number of subjects or number of occasions, the
  logical number can be set.

}
\usage{
nsubj(obj)
nsubj(obj) <- value
nocc(obj)
nocc(obj) <- value
maxocc(obj)
maxocc(obj) <- value
minocc(obj)
minocc(obj) <- value
}
\arguments{
  \item{obj}{A \code{\linkS4class{Panmat}}, \code{\linkS4class{Panel_Frame}} or
    \code{\link{Panel_Data}} object.}
  \item{value}{An integer giving the new value for the statistic.}
}
\value{
  An integer giving the number of subjects or occasions, or the minimum and/or maximum index.
}
\author{Russell Almond}
\seealso{
  \code{\linkS4class{Panmat}}, \code{\linkS4class{Panel_Frame}}
    \code{\link{Panel_Data}}
}
\examples{
}
\keyword{ classes }
\concept{ panel_data }

